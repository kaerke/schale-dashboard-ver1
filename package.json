    "@types/express": "^4.17.21",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.1.2",
    "tsx": "^4.19.2",
